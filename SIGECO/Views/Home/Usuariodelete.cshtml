<script>

// Form object
var CurrentPageID = ew.PAGE_ID = "delete";
var fUsuariodelete = currentForm = new ew.Form("fUsuariodelete", "delete");

// Form_CustomValidate event
fUsuariodelete.Form_CustomValidate = function(fobj) { // DO NOT CHANGE THIS LINE!

