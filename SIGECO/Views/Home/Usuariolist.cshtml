@Html.Raw(Usuario.nActivo.GetViewValue())</span>
</span>
</td>
	} @* nActivo.Visible (end) *@
	@{

		// Render list options (body, right)
		Usuario_List.ListOptions.Render("body", "right", Usuario_List.RowCnt);
	}
	</tr>
	} @* End if *@
} @* End While *@
} @* End Code *@
</tbody>
